<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.katri.lolvotingsystem"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="26" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/vslogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.katri.lolvotingsystem.MobileLogin" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.katri.lolvotingsystem.MobileVotingList" />
        <activity android:name="com.example.katri.lolvotingsystem.MobileCandidateList" />
        <activity android:name="com.example.katri.lolvotingsystem.AdminMenu" />
        <activity android:name="com.example.katri.lolvotingsystem.MobileResults" />
        <activity android:name="com.example.katri.lolvotingsystem.MobileSuccessSend" />
        <activity android:name="com.example.katri.lolvotingsystem.AddUser" />
        <activity
            android:name="com.example.katri.lolvotingsystem.RemoveUser"
            android:label="@string/title_activity_remove_user"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.katri.lolvotingsystem.RemoveVote" />
        <activity android:name="com.example.katri.lolvotingsystem.AddVote" />

        <meta-data
            android:name="android.support.VERSION"
            android:value="26.1.0" />
        <meta-data
            android:name="android.arch.lifecycle.VERSION"
            android:value="27.0.0-SNAPSHOT" />
    </application>

</manifest>